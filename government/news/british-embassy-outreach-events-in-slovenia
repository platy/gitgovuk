  <div class="govuk-grid-column-two-thirds content-bottom-margin">
    <div class="responsive-bottom-margin">
      <figure class="app-c-figure" lang="en">
  <img class="app-c-figure__image" src="https://assets.publishing.service.gov.uk/government/assets/placeholder-a6e35fc15d4bfcc9d0781f3888dc548b39288f0fe10a65a4b51fef603540b0c5.jpg" alt="placeholder"></img>
